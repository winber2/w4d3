<tr>
  <td><%= User.find_by(id: req.user.id).username %></td>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? %>
      <% if current_user.id == @cat.owner.id %>
          <%= button_to "Approve", approve_cat_rental_request_url(req) %>
          <%= button_to "Deny", deny_cat_rental_request_url(req) %>
        <% end %>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
</tr>
